{
    "services": {
        "cnc": {
            "script": {
                "remotePath": "/opt/scripts/startup",
                "localPath": "../../../../scripts/startup",
                "commandAndControl": "callhome.sh",
                "cleanup": "cleanup.sh"
            },

            "service": {
                "remotePath": "/etc/systemd/system",
                "localPath": "../../../../services",
                "commandAndControl": "callhome.service",
                "timer": "callhome-persistence.timer",
                "cleanup": "callhome-cleanup.service"
            },

            "key": {
                "remotePath": "/root/.ssh",
                "localPath": "../../../../.ssh",
                "currentConfig": "id_rsa"
            },

            "defaults": {
                "remoteServer": "CLOUD_C2",
                "tunnelPort": "2222",
                "monitorPort": "0"
            }
        },
        
        "ecrypt": {
            "script": {
                "remotePath": "/opt/scripts/startup",
                "localPath": "../../../../scripts/startup",
                "ecryptfs": "ecrypt.sh",
                "umountfs": "ecrypt-umount.sh"
            },

            "service": {
                "remotePath": "/etc/systemd/system",
                "localPath": "../../../../services",
                "ecryptfs": "ecrypt-persistence.service"
            }
        },

        "netfilter": {
            "script": {
                "remotePath": "/opt/scripts/startup",
                "localPath": "../../../../scripts/startup",
                "iptables": "iptables.sh"
            },

            "service": {
                "remotePath": "/etc/systemd/system",
                "localPath": "../../../../services",
                "iptables": "iptables-persistent.service"
            },

            "conf": {
                "remotePath": "/etc/iptables",
                "localPath": "../../../../conf",
                "rules": "rules.v4"
            }
        },

        "knockd": {
            "script": {
                "remotePath": "/opt/scripts/startup",
                "localPath": "../../../../scripts/startup",
                "knockd": "knock.sh"
            },

            "service": {
                "remotePath": "/etc/systemd/system",
                "localPath": "../../../../services",
                "knockd": "knockd.service",
                "ifaceChecker": "knockd-checker.service"
            },

            "conf": {
                "remotePath": "/etc",
                "localPath": "../../../../conf",
                "template": "knockd.conf"
            }
        },

        "resolved": {
            "conf": {
                "remotePath": "/etc/systemd",
                "localPath": "../../../../conf",
                "template": "resolved.conf"
            }
        },

        "sshd": {
            "conf": {
                "remotePath": "/etc/ssh",
                "localPath": "../../../../conf",
                "template": "sshd_config"
            }
        },

        "timesyncd": {
            "conf": {
                "remotePath": "/etc/systemd",
                "localPath": "../../../../conf",
                "template": "timesyncd.conf"
            }
        }
    }
}
